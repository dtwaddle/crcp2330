 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=a, out=a2n);
    Nand(a=b, b=b, out=b2n);
    Nand(a=a2n, b=b2n, out=out);

}
